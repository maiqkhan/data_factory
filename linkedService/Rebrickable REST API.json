{
	"name": "Rebrickable REST API",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"description": "This service is to connect to Rebrickable's V3 REST API.",
		"parameters": {
			"base_url_endpoint": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "RestService",
		"typeProperties": {
			"url": "@{linkedService().base_url_endpoint}",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous",
			"authHeaders": {
				"Authorization": {
					"type": "SecureString",
					"value": "**********"
				}
			}
		}
	}
}